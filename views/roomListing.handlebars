<section class="room-list">
    {{#each rooms }}
        <article>
            <header>
                <img src={{this.image}}>
            </header>
            <a href="#"><h1>{{this.name}}</h1></a>
            <ul>
                <li><h2>${{this.price}} CAD/night</h2></li>
                <li>{{this.description}}</li>
                <li>Bed Size: {{this.size}}</li>
                <li>{{this.rating}}</li>
            </ul>
        </article>
    {{/each}}
</section>